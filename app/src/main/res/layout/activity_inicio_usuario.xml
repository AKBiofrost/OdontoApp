<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:background="@color/wallpaper"
        android:layout_height="match_parent"
        tools:context=".InicioUsuario">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/SplashScreen"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="31dp"
            android:layout_marginEnd="21dp"
            android:layout_marginBottom="31dp"
            android:background="@drawable/app_wallpaper"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="227dp"
                android:layout_height="312dp"
                android:background="@drawable/app_marco_interno_dashboard"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.503"
                android:id="@+id/constraintLayout3" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="192dp">
            <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/wallpaper"
                    app:srcCompat="@drawable/btn_diente_molar"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginBottom="16dp"
                    android:id="@+id/btn_login"/>
            <EditText
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:text="@string/usuario"
                    android:ems="10"
                    android:textColor="@color/texto"
                    android:textSize="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:id="@+id/editText"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="45dp"
                    android:layout_marginBottom="44dp" app:layout_constraintBottom_toTopOf="@+id/editText2"/>
            <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="textPassword"
                    android:textColor="@color/texto"
                    android:text="@string/contrase_a"
                    android:textSize="20dp"
                    android:gravity="center"
                    android:ems="10"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:id="@+id/editText2"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <TextView
                android:text="ayuda"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ayuda"
                android:textSize="20dp"
                android:textColor="@color/texto"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="46dp"
                android:layout_marginBottom="51dp"/>
        <ImageButton
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"
                android:background="@color/wallpaper"
                app:srcCompat="@drawable/ic_manage_accounts_white_18dp"
                android:id="@+id/btn_registro"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginEnd="54dp"
                android:layout_marginBottom="45dp"
                app:layout_constraintEnd_toEndOf="parent"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/app_usuario"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:id="@+id/imageView"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="26dp"/>


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>